}
.divLine{
    margin-left: 50rpx;
    background: rgb(192, 190, 190);
    width: 100%;
    height: 2rpx;
  
  }
  .photo_box{
    width: 750rpx;
    border: 1px solid #cccccc;
    box-sizing: border-box;
  }
  .photo_text{
    width: 100%;
